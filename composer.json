{
  "name": "piotrpress/composer-streams",
  "type": "composer-plugin",
  "license": "MIT",
  "description": "This composer plugin adds github and bitbucket streams support to Composer in order to simplify private repositories handling.",
  "homepage": "https://PiotrPress",
  "authors": [
    {
      "name": "Piotr Niewiadomski",
      "homepage": "https://github.com/PiotrPress"
    }
  ],
  "keywords": [
    "composer", "composer plugin", "stream", "streams", "github", "bitbucket", "php"
  ],
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^2.0",
    "piotrpress/streamer": "^1.0",
    "piotrpress/remoter": "^1.0"
  },
  "extra": {
    "class": "PiotrPress\\Composer\\Streams\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "PiotrPress\\Composer\\Streams\\": "src/"
    },
    "classmap": [
      "src"
    ]
  }
}